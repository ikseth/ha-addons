{
  "name": "Log2Syslog Add-on",
  "version": "1.1.5",
  "slug": "log2syslog",
  "description": "Addon that forwards home-assistant.log to a remote syslog server using syslog-ng.",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "host_network": false,
  "options": {
    "remote_host": "192.168.50.62",
    "remote_port": 514,
    "facility": "local5"
  },
  "schema": {
    "remote_host": "str",
    "remote_port": "int",
    "facility": "str"
  }
}
